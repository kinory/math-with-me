<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="kkkk.mathwithme"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="17"
        android:targetSdkVersion="23" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name="kkkk.mathwithme.activity.MenuActivity"
            android:screenOrientation="landscape" />
        <activity
            android:name="kkkk.mathwithme.activity.LoginActivity"
            android:screenOrientation="landscape" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="kkkk.mathwithme.activity.SignUpActivity"
            android:screenOrientation="landscape" />
        <activity
            android:name="kkkk.mathwithme.activity.ProfileActivity"
            android:screenOrientation="landscape" />
    </application>

</manifest>